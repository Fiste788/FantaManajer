@media (max-width: 480px) {
  body.debug::before {
    content: "Mobile media query (0 < 480) fired";
  }
  .table th,
  .table td {
    padding: 4px 5px;
  }
  #content {
    min-height: auto !important;
  }
}
@media (max-width: 768px) {
  nav#topbar ul.pull-right {
    float: none !important;
    max-width: none !important;
    width: 100% !important;
  }
}
@media (min-width: 768px) {
